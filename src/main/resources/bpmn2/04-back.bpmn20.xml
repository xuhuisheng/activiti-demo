    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement='transition-4' id='BPMNShape_transition-4'>
      <omgdi:waypoint x='355' y='55'/>
      <omgdi:waypoint x='385' y='55'/>
    </bpmndi:BPMNEdge>
  </bpmndi:BPMNPlane>
